{
	"env": {
		"browser": true,
		"es2021": true
	},
	"extends": [
		"eslint:recommended",
		"plugin:vue/vue3-recommended",
		"plugin:@typescript-eslint/recommended",
		"plugin:nuxt/recommended",
		"prettier",
		"plugin:prettier/recommended"
	],
	"overrides": [],
	"parser": "vue-eslint-parser",
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module",
		"parser": "@typescript-eslint/parser"
	},
	"plugins": ["vue", "@typescript-eslint", "prettier"],
	"rules": {
		"indent": "off",
		"linebreak-style": ["error", "unix"],
		"quotes": ["error", "double"],
		"semi": ["error", "always"],
		"prettier/prettier": ["error"],
		"vue/multi-word-component-names": "off",
		"@typescript-eslint/no-unused-vars": "off"
	},
	"globals": {
		"_": "readonly",
		"$fetch": "readonly",
		"clearError": "readonly",
		"computed": "readonly",
		"defineNuxtPlugin": "readonly",
		"definePageMeta": "readonly",
		"Intercom": "readonly",
		"navigateTo": "readonly",
		"onBeforeMount": "readonly",
		"onMounted": "readonly",
		"onUnmounted": "readonly",
		"onBeforeUnmount": "readonly",
		"provide": "readonly",
		"ref": "readonly",
		"resolveComponent": "readonly",
		"Stripe": "readonly",
		"toRef": "readonly",
		"useAuth": "readonly",
		"useCookie": "readonly",
		"useError": "readonly",
		"useHead": "readonly",
		"useNuxtApp": "readonly",
		"useOnboardingNavigation": "readonly",
		"useResetOnboarding": "readonly",
		"useRoute": "readonly",
		"useRouter": "readonly",
		"useRuntimeConfig": "readonly",
		"useStore": "readonly",
		"useSupabaseClient": "readonly",
		"useSupabaseUser": "readonly",
		"useToast": "readonly",
		"useUtilityIntegration": "true",
		"useUtils": "readonly",
		"watch": "readonly"
	}
}
